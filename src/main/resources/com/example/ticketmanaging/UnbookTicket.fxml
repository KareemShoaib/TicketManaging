<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="760.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ticketmanaging.UnbookTicketController">
    <children>
        <Label layoutX="35.0" layoutY="165.0" prefHeight="26.0" prefWidth="168.0" text="Choose Client Name :">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <ComboBox fx:id="clientsBox" layoutX="214.0" layoutY="165.0" onAction="#AddToDropDownEVENTS" prefHeight="26.0" prefWidth="518.0" promptText="Choose Client" />
        <Button layoutX="95.0" layoutY="421.0" mnemonicParsing="false" onAction="#BACK" prefHeight="32.0" prefWidth="570.0" style="-fx-background-color: #000000;" text="Back" textFill="WHITE">
            <font>
                <Font size="21.0" />
            </font>
        </Button>
        <Label alignment="TOP_CENTER" layoutX="265.0" layoutY="52.0" prefHeight="32.0" prefWidth="230.0" text="UNBOOK TICKET" textAlignment="CENTER">
            <font>
                <Font name="System Bold Italic" size="22.0" />
            </font>
        </Label>
        <Button fx:id="Unbook" layoutX="95.0" layoutY="362.0" mnemonicParsing="false" onAction="#UnbookTicket" prefHeight="32.0" prefWidth="570.0" style="-fx-background-color: #000000;" text="Unbook" textFill="WHITE">
            <font>
                <Font size="21.0" />
            </font>
        </Button>
        <Label fx:id="VERIFY" alignment="TOP_CENTER" layoutX="335.0" layoutY="145.0" prefHeight="18.0" prefWidth="276.0" style="-fx-text-fill: black;" textAlignment="CENTER">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label layoutX="85.0" layoutY="213.0" prefHeight="26.0" prefWidth="168.0" text="Choose Event :">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <ComboBox fx:id="eventsBox" layoutX="213.0" layoutY="214.0" prefHeight="26.0" prefWidth="518.0" promptText="Events" />
    </children>
</AnchorPane>
